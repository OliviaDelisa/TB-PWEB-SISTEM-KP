

<h2 class="title">Form Tambah Perusahaan</h2>

<form action="/perusahaan/tambah" method="POST" class="form-perusahaan">
  <div class="form-group">
    <label for="nama_perusahaan">Nama Perusahaan</label>
    <input type="text" name="nama_perusahaan" required>
  </div>

  <div class="form-group">
    <label for="alamat_perusahaan">Alamat</label>
    <input type="text" name="alamat_perusahaan">
  </div>

  <div class="form-group">
    <label for="email_perusahaan">Email</label>
    <input type="email" name="email_perusahaan">
  </div>

  <div class="form-group">
    <label for="kontak">Kontak</label>
    <input type="text" name="kontak">
  </div>

  <div class="form-group">
    <label for="kategori">Kategori</label>
    <input type="text" name="kategori">
  </div>

  <div class="form-group">
    <label for="kuota">Kuota</label>
    <input type="number" name="kuota">
  </div>

  <div class="form-buttons">
    <a href="/perusahaan" class="btn-cancel">Batal</a>
    <button type="submit" class="btn-submit">Simpan</button>
  </div>
</form>
